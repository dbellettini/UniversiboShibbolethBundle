{
    "name": "dbellettini/shibboleth-bundle",
    "type": "symfony-bundle",
    "description": "Shibboleth Integration for FOSUserBundle",
    "keywords": [
        "authentication",
        "shibboleth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bellettini",
            "email": "davide.bellettini@gmail.com"
        },
        {
            "name": "ShibbolethBundle Contributors",
            "homepage": "https://github.com/dbellettini/UniversiboShibbolethBundle/graphs/contributors"
        }
    ],
    "require": {
        "friendsofsymfony/user-bundle": "*",
        "symfony/framework-bundle": "~2.1",
        "symfony/security-bundle": "~2.1",
        "zendframework/zend-uri": "~2.0"
    },
    "require-dev": {
        "symfony/yaml": "~2.3",
        "phpunit/phpunit": "~3.7"
    },
    "replace": {
        "universibo/shibboleth-bundle": "self.version"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Universibo\\Bundle\\ShibbolethBundle": ""
        }
    },
    "target-dir": "Universibo/Bundle/ShibbolethBundle"
}
